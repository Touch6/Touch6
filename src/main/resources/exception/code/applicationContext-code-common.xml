<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:error="http://www.heqmentor.com/schema/error"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
                           http://www.heqmentor.com/schema/error
                           http://www.heqmentor.com/schema/error/error.xsd">

    <!-- 异常代码配置  -->
    <bean id="eCode_comm" class="com.heqmentor.core.exception.ECode">
        <property name="errorMap">
            <!--suppress InjectionValueTypeInspection -->
            <map>
                <!--20005 配置模块-->
                <entry key="common.configNotFound">
                    <error:error code="2000501" info="配置信息不存在" des="必要配置信息未配置"/>
                </entry>
            </map>
        </property>
    </bean>
</beans>
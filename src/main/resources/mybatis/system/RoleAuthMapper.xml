<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- namespace必须指向Dao接口 -->
<mapper namespace="com.touch6.business.mybatis.system.RoleAuthMybatisDao">
    <insert id="insertRoleAuth" parameterType="com.touch6.business.entity.system.RoleAuth">
        insert ignore into t_si_role_auth(
        role_id,auth_id
        )
        values(
        #{roleId},#{authId}
        )
    </insert>
</mapper>